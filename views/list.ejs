<%- include("header") -%>
    <div class="box" id="heading">
        <h1><%=Wday%></h1>
    </div>

    <div class="box">
            <% for(var i=0;i<newListItem.length;i++) {%>
                <div class="item">
                    <input type="checkbox" >
                    <p><%= newListItem[i] %></p>
                </div>
            <% } %>
            <form action="/" method="post" class="item">
                <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
                <button type="submit" name="list" value=<%= Wday%>>+</button>
            </form>
    </div>
    
<%- include("footer") -%>
    